{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.scss"
    ],
    "customizer": [
      "./scripts/customizer.js"
    ]
  },
  "globals": {
    "wp": true,
    "admin_url": true
  },
  "publicPath": "/wp-content/themes/writerWorks",
  "devUrl": "http://localhost:8888/site",
  "proxyUrl": "http://localhost:4000",
  "cacheBusting": "[name]_[hash:8]",
  "watch": [
    "app/**/*.php",
    "config/**/*.php",
    "resources/views/**/*.php"
  ]
}
